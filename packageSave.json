{
  "name": "@tntx/nested-table",
  "version": "1.0.10",
  "description": "基于ant3开发的嵌套表格，用于弥补<Table>@ant3中 expandedRowRender={return <Table>} 时无法同时使用Column.fixed='right'的问题。参考官方提示(Warning: [antd: Table] `expandedRowRender` and `Column.fixed` are not compatible. Please use one of them at one time.)",
  "main": "lib/index.js",
  "scripts": {
    "start": "SYS_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "SYS_ENV=production webpack --progress --config build/webpack.prod.conf.js",
    "dll": "webpack --config ./build/webpack.dll.conf.js",
    "lint": "eslint --ext .js,.vue src",
    "build2": "npx babel src/components/NestedTable --out-dir lib --copy-files",
    "publish": "sh publish.sh"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ant3",
    "table",
    "ant table",
    "ant3 table",
    "Table",
    "expandedRow",
    "expandedRowRender",
    "fixed",
    "Column.fixed",
    "scroll",
    "nested table",
    "@tntx/nested-table"
  ],
  "author": "theYoungBoyLHG",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-tdued": "^1.1.4",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@rematch/core": "^1.2.0",
    "antd": "^3.26.18",
    "axios": "^0.19.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "kityminder-core": "^1.4.50",
    "qs": "^6.9.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-kityminder": "^0.5.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "use-resize-observer": "^7.0.0",
    "uuid": "^8.3.2"
  }
}
